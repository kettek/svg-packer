{
  "name": "svg-packer",
  "version": "0.0.0",
  "description": "Pack multiple SVGs into a single SVG as symbols for use ON THE WEB.",
  "main": "index.js",
  "scripts": {
    "build": "lasso --main ./app/client.js --plugins lasso-marko --inject-into ./index.html",
    "watch-build": "lasso --main ./app/client.js --plugins lasso-marko --inject-into ./index.html --watch"
  },
  "keywords": ["svg", "symbol", "packer"],
  "author": "Ketchetwahmeegwun T. Southall / kts of kettek",
  "license": "GPL-3.0",
  "dependencies": {
    "lasso-marko": "^2.4.7",
    "marko": "^4.18.11"
  },
  "devDependencies": {
    "lasso-cli": "^2.0.11"
  }
}
